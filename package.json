{
  "name": "baked-frame",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parallelshell \"npm run watch:js\" \"npm run watch:css\"",
    "build": "npm run build:js && npm run build:css",
    "watch:css": "onchange 'demo.css' -- npm run build:css",
    "watch:js": "budo demo.js:bundle.js -v --live -- -t [babelify --presets [es2015] ] -t [ installify --save ] -t [glslify]",
    "build:js": "browserify demo.js -g uglifyify -t [babelify --presets [es2015] ] -t [ installify --save ] -t [glslify]  -o bundle.js",
    "build:css": "styl -w < demo.css > bundle.css"
  },
  "author": "Hiroaki Yamane",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.1",
    "budo": "^10.0.0",
    "installify": "^1.0.2",
    "onchange": "^3.0.0",
    "parallelshell": "^3.0.0",
    "styl": "mnmly/styl",
    "uglifyify": "^4.0.5"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "gl-vec2": "^1.0.0",
    "gl-vec3": "^1.0.3",
    "gl-vec4": "^1.0.1",
    "glslify": "^6.0.0",
    "gsap": "^1.20.3",
    "hammerjs": "^2.0.8",
    "keycode": "^2.1.9",
    "raf-loop": "^1.1.3",
    "remap": "mnmly/remap",
    "resl": "^1.0.3",
    "three": "^0.88.0",
    "three-orbit-controls": "^82.1.0",
    "webworkify": "^1.5.0",
    "xhr": "^2.4.1"
  }
}
